            <div class="deux-col-50-50">
                <div class="col-A">
                    <div class="ressources">
                        <h3><i class="fas fa-euro-sign"></i> RESSOURCES</h3>
                        <div class="ress-grid">
                            <!-- TITRE -->
                            <div class="col-titre-vide"></div>
                            <div class="col-titre diminution"><label>Diminution</label></div>
                            <div class="col-titre duree"><label>Durée</label></div>
                            <!-- RICHESSE -->
                            <div class="colRess-dice"><a class="roll-ress" title="{{localize 'TRINITES.label.jetRessource'}}" data-ressource="richesse"><i class="fas fa-dice"></i></a></div>
                            <div class="colRess-nom">{{localize 'TRINITES.label.ressources.richesse'}}</div>
                            <div class="colRess-epuisee">
                                {{#if data.system.ressources.richesse.epuisee }}
                                    <div class="uncheck-richesse"><a title="Richesse épuisée"><i class="far fa-check-square"></i></a></div>
                                {{else}}
                                    <div class="check-richesse"><a title="Richesse épuisée"><i class="far fa-square"></i></a></div>
                                {{/if}}
                            </div>
                            <div class="colRess-valeur"><input name="system.ressources.richesse.valeur" type="number" value="{{data.system.ressources.richesse.valeur}}" placeholder="0" min="0" {{#unless unlocked}}disabled {{/unless}}></div>
                            <div class="colRess-diminution"><input name="system.ressources.richesse.diminution" type="number" value="{{data.system.ressources.richesse.diminution}}" placeholder="0" min="0" max="{{data.system.ressources.richesse.valeur}}"></div>                
                            <div class="colRess-duree"><input name="system.ressources.richesse.duree" type="text" value="{{data.system.ressources.richesse.duree}}" placeholder="Durée diminution"></div>
                            <!-- RESEAU -->
                            <div class="colRess-dice"><a class="roll-ress" title="{{localize 'TRINITES.label.jetRessource'}}" data-ressource="reseau"><i class="fas fa-dice"></i></a></div>
                            <div class="colRess-nom">{{localize 'TRINITES.label.ressources.reseau'}}</div>
                            <div class="colRess-valeur"><input name="system.ressources.reseau.valeur" type="number" value="{{data.system.ressources.reseau.valeur}}" placeholder="0" min="0" {{#unless unlocked}}disabled {{/unless}}></div>
                            <div class="colRess-diminution"><input name="system.ressources.reseau.diminution" type="number" value="{{data.system.ressources.reseau.diminution}}" placeholder="0" min="0" max="{{data.system.ressources.reseau.valeur}}"></div>                
                            <div class="colRess-duree"><input name="system.ressources.reseau.duree" type="text" value="{{data.system.ressources.reseau.duree}}" placeholder="Durée diminution"></div>
                            <!-- INFLUENCE -->
                            <div class="colRess-dice"><a class="roll-ress" title="{{localize 'TRINITES.label.jetRessource'}}" data-ressource="influence"><i class="fas fa-dice"></i></a></div>

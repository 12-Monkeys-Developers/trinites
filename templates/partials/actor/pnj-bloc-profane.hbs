                <div class="col-B">
                    <div class="lignes-de-vie">
                        <h3><i class="fas fa-heart"></i> LIGNES DE VIE</h3>
                        {{#if unlocked}}
                            <div class="param"><label>Nb lignes de vie : </label>
                            <select name="system.nbLigneVie">
                                {{selectOptions config.cout_1_3 selected=data.system.nbLigneVie}}
                                </select>
                                </div>
                            <div class="param"><label>Points de vie par ligne :</label><input name="system.pointsLigneVie" type="number" value="{{data.system.pointsLigneVie}}"></div>
                        {{/if}}
                        <div class="ligne-vie">
                            {{#times data.system.nbPointsVieMax}}
                                <a class="case-vie" title="{{@index}}" data-index="{{@index}}">{{#if (gte data.system.nbBlessure @index)}}<i class="fas fa-circle"></i>{{else}}<i class="far fa-circle"></i>{{/if}}</a>
                                {{#if (eq @index data.system.ligneVie1)}}</div><div class="ligne-vie">{{/if}}
                                {{#if (eq @index data.system.ligneVie2)}}</div><div class="ligne-vie">{{/if}}
                            {{/times}}
                        </div>
                        <div class="etat-sante">
                            <label>Etat de santé : {{configVal 'etatSante' data.system.etatSante}} </label>
                            {{#if peutRegenerer}}
                                <div class="regen-btn"><a class="regen" title="Régénération par le Karma"><i class="fas fa-hand-holding-medical"></i></a></div>
                            {{/if}}
                        </div>
                    </div>    
                </div>            
